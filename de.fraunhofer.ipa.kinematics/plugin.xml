<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated urdf -->
      <package
            uri="http://www.ipa.fraunhofer.de/urdf"
            class="urdf.UrdfPackage"
            genModel="model/urdf.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated urdf -->
      <parser
            type="urdf"
            class="urdf.util.UrdfResourceFactoryImpl"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated xacro -->
      <package
            uri="http://www.ipa.fraunhofer.de/xacro"
            class="xacro.XacroPackage"
            genModel="model/composition.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated kinematics -->
      <package
            uri="http://www.ipa.fraunhofer.de/kinematics"
            class="kinematics.KinematicsPackage"
            genModel="model/kinematics.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated composition -->
      <package
            uri="http://www.ipa.fraunhofer.de/composition"
            class="composition.CompositionPackage"
            genModel="model/composition.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated composition -->
      <parser
            type="composition"
            class="composition.util.CompositionResourceFactoryImpl"/>
   </extension>
</plugin>
